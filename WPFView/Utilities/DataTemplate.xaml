<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WPFView.ViewModel"
                    xmlns:view="clr-namespace:WPFView.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FitnessClubsVM}">
        <view:FitnessClubs/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MembershipsVM}">
        <view:Memberships/>
    </DataTemplate>

</ResourceDictionary>